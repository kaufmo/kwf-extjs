{
    "name": "koala-framework/kwf-extjs",
    "description": "ExtJS Integration for Koala Framework",
    "homepage": "http://www.koala-framework.org/",
    "license": "BSD-2-Clause",
    "type": "library",
    "require": {
        "koala-framework/koala-framework": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "extra": {
        "require-bower": {
            "extjs": "5.0.1-1255"
        },
        "require-dev-bower": {
            "qunit": "1.14.0",
            "qunit-phantomjs-runner": "jonkemp/qunit-phantomjs-runner#1.2.0"
        }
    },
    "autoload": {
        "psr-0": {
            "Kwf_Ext_": ""
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Kwf_Ext_": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
