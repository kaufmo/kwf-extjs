{
    "name": "koala-framework/kwf-extjs",
    "description": "ExtJS Integration for Koala Framework",
    "homepage": "http://www.koala-framework.org/",
    "license": "BSD-2-Clause",
    "type": "library",
    "require": {
        "koala-framework/koala-framework": "~3.9"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "extra": {
        "require-bower": {
            "extjs": "4.2.1-883",
            "moxie": "1.2.1"
        },
        "require-dev-bower": {
            "qunit": "1.14.0",
            "qunit-phantomjs-runner": "vivid-planet/qunit-phantomjs-runner#e96347ee33b773160cf061d7b1b5c4ad3d1d9082"
        }
    },
    "autoload": {
        "psr-0": {
            "Kwf_Ext_": ""
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Kwf_Ext_": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
